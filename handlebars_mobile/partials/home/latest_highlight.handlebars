<div class="feed-item">
	<div class="feed-asset">
		<a href="{{channel.link}}" class="feed-link">
			<img src="https://asset.kompas.com/crop/0x0:0x0/600x600/data/photo/{{channel.img}}" alt="{{channel.title}}">
		</a>
	</div>
	<div class="feed-box">
		<h3 class="feed-title"><a href="{{channel.link}}" class="feed-link">{{channel.title}}</a></h3>
		{{#eq author compare=true}}
		<span class="feed-author">by: <a href="indeks-author.html">{{channel.author}}</a></span>
		{{/eq}}
	</div>
</div>
